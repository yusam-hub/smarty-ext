{
    "name": "yusam-hub/smarty-ext",
    "description": "yusam-hub/smarty-ext",
    "type": "package",
    "minimum-stability": "stable",
    "repositories": {
        "yusam-hub/php-compatibility": {
            "type": "git",
            "url": "https://github.com/yusam-hub/php-compatibility.git"
        }
    },
    "require": {
        "php": ">=7.4 <9.0",
        "smarty/smarty": "^5.7",
        "yusam-hub/php-compatibility": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "YusamHub\\SmartyExt\\": "src/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YusamHub\\SmartyExt\\Tests\\": "tests/"
        },
        "files": [
        ]
    }
}
